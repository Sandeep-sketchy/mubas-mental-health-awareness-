<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear_parent"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#FFFFFF"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/base"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/trash"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/linear_page_box"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/layout1"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="#F44336"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/bg1"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/bg"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:background="#FFFFFF"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/top"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical"
							android:layout_weight="1">
							<LinearLayout
								android:id="@+id/linear2"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal"
								android:layout_weight="2"/>
							<LinearLayout
								android:id="@+id/linear3"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:orientation="vertical">
								<TextView
									android:id="@+id/textview1"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginLeft="25dp"
									android:text="Take Care of "
									android:textSize="25sp"
									android:textColor="#000000"/>
								<TextView
									android:id="@+id/textview4"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginLeft="25dp"
									android:text="Your Mental Health!!!"
									android:textSize="20sp"
									android:textColor="#000000"/>
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear4"
								android:layout_width="match_parent"
								android:layout_height="50dp"
								android:padding="8dp"
								android:orientation="horizontal"/>
							<LinearLayout
								android:id="@+id/linear5"
								android:layout_width="180dp"
								android:layout_height="53dp"
								android:layout_marginLeft="25dp"
								android:background="#5B65F5"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<TextView
									android:id="@+id/textview5"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text=" NEXT》"
									android:textSize="14sp"
									android:textColor="#FFFFFF"/>
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear6"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal"
								android:layout_weight="2"/>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/bottom"
							android:layout_width="match_parent"
							android:layout_height="400dp"
							android:gravity="bottom"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview1"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:src="@drawable/stress_2"
								android:scaleType="fitXY"/>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/layout2"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="#2196F3"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear7"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/linear8"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:background="#FFFFFF"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear9"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical"
							android:layout_weight="1">
							<LinearLayout
								android:id="@+id/linear11"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal"
								android:layout_weight="2"/>
							<LinearLayout
								android:id="@+id/linear12"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:orientation="vertical">
								<TextView
									android:id="@+id/textview6"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginLeft="25dp"
									android:text="Get the Help You Need."
									android:textSize="25sp"
									android:textColor="#000000"/>
								<TextView
									android:id="@+id/textview7"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginLeft="25dp"
									android:text="We got you covered!!"
									android:textSize="20sp"
									android:textColor="#000000"/>
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear13"
								android:layout_width="match_parent"
								android:layout_height="50dp"
								android:padding="8dp"
								android:orientation="horizontal"/>
							<LinearLayout
								android:id="@+id/linear14"
								android:layout_width="180dp"
								android:layout_height="53dp"
								android:layout_marginLeft="25dp"
								android:background="#5B65F5"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<TextView
									android:id="@+id/textview2"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="Self care comes first 》"
									android:textSize="14sp"
									android:textColor="#FFFFFF"/>
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear15"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal"
								android:layout_weight="2"/>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear10"
							android:layout_width="match_parent"
							android:layout_height="400dp"
							android:gravity="bottom"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview2"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:src="@drawable/stress_1"
								android:scaleType="fitXY"/>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/layout3"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="#4CAF50"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear16"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/linear17"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:background="#FFFFFF"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear18"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical"
							android:layout_weight="1">
							<LinearLayout
								android:id="@+id/linear20"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal"
								android:layout_weight="2"/>
							<LinearLayout
								android:id="@+id/linear21"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:orientation="vertical">
								<TextView
									android:id="@+id/textview8"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginLeft="25dp"
									android:text="Let's Talk About It!!"
									android:textSize="25sp"
									android:textColor="#000000"/>
								<TextView
									android:id="@+id/textview3"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginLeft="25dp"
									android:text="Connect with the community"
									android:textSize="20sp"
									android:textColor="#000000"/>
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear22"
								android:layout_width="match_parent"
								android:layout_height="50dp"
								android:padding="8dp"
								android:orientation="horizontal"/>
							<LinearLayout
								android:id="@+id/linear23"
								android:layout_width="180dp"
								android:layout_height="53dp"
								android:layout_marginLeft="25dp"
								android:background="#5B65F5"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<TextView
									android:id="@+id/textview9"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="Get Started  》"
									android:textSize="14sp"
									android:textColor="#FFFFFF"/>
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear24"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal"
								android:layout_weight="2"/>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear19"
							android:layout_width="match_parent"
							android:layout_height="400dp"
							android:gravity="bottom"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview3"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:src="@drawable/stress_7"
								android:scaleType="fitXY"/>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear1"
			android:layout_width="match_parent"
			android:layout_height="50dp"
			android:paddingLeft="8dp"
			android:paddingRight="8dp"
			android:background="#8C9EFF"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:translationY="-50.0dp">
			<TextView
				android:id="@+id/txt_skip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="SKIP"
				android:textSize="12sp"
				android:textColor="#000000"
				android:singleLine="true"/>
			<LinearLayout
				android:id="@+id/linear_dots_box"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/dot1"
					android:layout_width="15dp"
					android:layout_height="10dp"
					android:src="@drawable/circle_filled"
					android:scaleType="fitCenter"/>
				<ImageView
					android:id="@+id/dot2"
					android:layout_width="15dp"
					android:layout_height="10dp"
					android:src="@drawable/circle_outline"
					android:scaleType="fitCenter"/>
				<ImageView
					android:id="@+id/dot3"
					android:layout_width="15dp"
					android:layout_height="10dp"
					android:src="@drawable/circle_outline"
					android:scaleType="fitCenter"/>
			</LinearLayout>
			<TextView
				android:id="@+id/txt_next"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="NEXT"
				android:textSize="12sp"
				android:textColor="#000000"
				android:singleLine="true"/>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
